{
    "name": "workingconcept/craft-snipcart",
    "description": "Do more with Snipcart directly from Craft CMS.",
    "type": "craft-plugin",
    "version": "1.0.0-beta.22",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft3",
        "snipcart"
    ],
    "support": {
        "docs": "https://snipcart.docs.workingconcept.com/",
        "issues": "https://github.com/workingconcept/snipcart-craft-plugin/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Working Concept",
            "homepage": "https://workingconcept.com"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "craftcms/cms": "^3.0.0",
        "pelago/emogrifier": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "workingconcept/yii2-apidoc": "@dev"
    },
    "autoload": {
        "psr-4": {
            "workingconcept\\snipcart\\": "src/"
        }
    },
    "repositories": [
        {
          "type": "git",
          "url": "https://github.com/workingconcept/yii2-apidoc"
        }
    ],
    "extra": {
        "name": "Snipcart",
        "handle": "snipcart",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://github.com/workingconcept/snipcart-craft-plugin/changelog.md",
        "class": "workingconcept\\snipcart\\Snipcart"
    }
}
